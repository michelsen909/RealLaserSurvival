<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kristoffermichelsen.reallasersurvival">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/laser_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity android:name=".GameOverActivity" />
        <activity android:name=".GameActivity" />
        <activity android:name=".HighscoreActivity" />
        <activity android:name=".GameActivity2"></activity>
        <service android:name="PlayAudio"  android:enabled="true"></service>
    </application>

</manifest>